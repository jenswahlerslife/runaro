{
  "name": "runaro",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "packageManager": "npm@10.9.2",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "cross-env ROLLUP_DISABLE_NATIVE=1 vite build",
    "build:dev": "vite build --mode development",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "preview": "vite preview",
    "sync": "node auto-sync.js",
    "sync:message": "node -e \"const {syncWithMessage} = require('./sync-helper'); syncWithMessage(process.argv[1])\"",
    "git:status": "git status",
    "git:push": "git add . && git commit -m 'Auto-update' && git push origin main",
    "cf:deploy": "node cloudflare-deploy.js",
    "cf:dev": "node cloudflare-dev.js",
    "cf:status": "npx wrangler pages deployment list --project-name runaro",
    "cf:logs": "npx wrangler pages deployment tail --project-name runaro",
    "deploy": "node deploy.js",
    "deploy:quick": "npm run build && npm run cf:deploy",
    "db:env": "powershell -Command \"Get-Content .env.local | Where-Object {$_ -notmatch '^#'} | ForEach-Object {$k,$v = $_.Split('=',2); [System.Environment]::SetEnvironmentVariable($k,$v,'Process')}\"",
    "db:status": "npm run db:env && npx supabase migration list",
    "db:push": "npm run db:env && npx supabase db push",
    "db:pull": "npm run db:env && npx supabase db pull",
    "db:new": "npx supabase migration new",
    "db:login": "npm run db:env && npx supabase login --token %SUPABASE_ACCESS_TOKEN%",
    "db:link": "npm run db:env && npx supabase link --project-ref %SUPABASE_PROJECT_REF%",
    "db:link:pw": "npm run db:env && npx supabase link --project-ref %SUPABASE_PROJECT_REF% -p %SUPABASE_DB_PASSWORD%",
    "db:setup": "npm run db:login && npm run db:link",
    "db:deploy": "npm run db:env && npm run db:push",
    "db:push:remote": "npm run db:env && npx supabase db push -p %SUPABASE_DB_PASSWORD% --include-roles",
    "functions:deploy": "npm run db:env && npx supabase functions deploy --project-ref %SUPABASE_PROJECT_REF%",
    "migration:test": "node scripts/migration-test-framework.cjs",
    "migration:validate": "npm run migration:test",
    "migration:health": "npm run db:env && node scripts/migration-test-framework.cjs",
    "performance:monitor": "node scripts/performance-monitor.cjs",
    "performance:track": "npm run performance:monitor",
    "db:performance": "npm run db:env && node scripts/performance-monitor.cjs",
    "v2:export": "node scripts/v2-schema-export.cjs",
    "v2:analyze": "npm run v2:export",
    "migration:v2-export": "npm run db:env && node scripts/v2-schema-export.cjs",
    "v2:rollback": "node scripts/v2-rollback-procedures.cjs",
    "v2:rollback-dry": "node scripts/v2-rollback-procedures.cjs --dry-run",
    "v2:test": "node scripts/v2-migration-test.cjs",
    "v2:validate": "npm run v2:test",
    "secrets:list": "node scripts/supabase-secrets-manager.cjs list",
    "secrets:check": "node scripts/supabase-secrets-manager.cjs secrets",
    "secrets:set": "node scripts/supabase-secrets-manager.cjs set-runaro",
    "secrets:fix-transfer": "node scripts/supabase-secrets-manager.cjs fix-transfer-activity",
    "edge:deploy": "node scripts/automated-edge-function-deploy.js deploy",
    "edge:deploy-all": "node scripts/automated-edge-function-deploy.js deploy-all",
    "edge:quick-fix": "node scripts/automated-edge-function-deploy.js quick-fix"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.10.0",
    "@radix-ui/react-accordion": "^1.2.11",
    "@radix-ui/react-alert-dialog": "^1.1.14",
    "@radix-ui/react-aspect-ratio": "^1.1.7",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-collapsible": "^1.1.11",
    "@radix-ui/react-context-menu": "^2.2.15",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-hover-card": "^1.1.14",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-menubar": "^1.1.15",
    "@radix-ui/react-navigation-menu": "^1.2.13",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-radio-group": "^1.3.7",
    "@radix-ui/react-scroll-area": "^1.2.9",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slider": "^1.3.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-toast": "^1.2.14",
    "@radix-ui/react-toggle": "^1.1.9",
    "@radix-ui/react-toggle-group": "^1.1.10",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@supabase/supabase-js": "^2.56.0",
    "@tanstack/react-query": "^5.83.0",
    "@types/leaflet": "^1.9.20",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^3.6.0",
    "dotenv": "^17.2.2",
    "embla-carousel-react": "^8.6.0",
    "input-otp": "^1.4.2",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.462.0",
    "next-themes": "^0.3.0",
    "node-fetch": "^3.3.2",
    "pg": "^8.16.3",
    "polyline": "^0.2.0",
    "react": "^18.3.1",
    "react-day-picker": "^8.10.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.61.1",
    "react-leaflet": "^4.2.1",
    "react-resizable-panels": "^2.1.9",
    "react-router-dom": "^6.30.1",
    "recharts": "^2.15.4",
    "sonner": "^1.7.4",
    "stripe": "^18.5.0",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7",
    "vaul": "^0.9.9",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@tailwindcss/typography": "^0.5.16",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.5.0",
    "@types/node": "^22.16.5",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "@vitejs/plugin-react-swc": "^3.11.0",
    "@vitest/coverage-v8": "^0.34.0",
    "@vitest/ui": "^0.34.0",
    "autoprefixer": "^10.4.21",
    "cross-env": "^10.0.0",
    "eslint": "^9.32.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^15.15.0",
    "jsdom": "^22.1.0",
    "lovable-tagger": "^1.1.9",
    "postcss": "^8.5.6",
    "prettier": "^3.0.0",
    "rollup": "^4.50.0",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.38.0",
    "vite": "^5.4.19",
    "vitest": "^0.34.0"
  }
}
